<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net6</TargetFramework>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>OpenSlideGTK</PackageId>
    <PackageTags>OpenSlide; GTK; </PackageTags>
    <PackageReleaseNotes>Cross platform Windows, Linux &amp; Mac .NET6 bindings for OpenSlide(https://openslide.org/), including DeepZoomGenerator</PackageReleaseNotes>
    <Description>.NET6 bindings for OpenSlide(https://openslide.org/), including DeepZoomGenerator</Description>
    <Version>1.1.0</Version>
    <Title>OpenSlideGTK</Title>
    <Authors>Erik Repo; IOL0ol1;</Authors>
    <PackageIcon>openslide_logo.png</PackageIcon>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/BiologyTools/OpenSlideSharp</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
  </PropertyGroup>
  <ItemGroup>
    <None Include="..\..\openslide_logo.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>
  
  <ItemGroup>
    <PackageReference Include="AForgeBio" Version="1.4.3" />
    <PackageReference Include="BruTile" Version="5.0.6" />
    <PackageReference Include="Mapsui" Version="4.1.1" />
    <PackageReference Include="Mapsui.Tiling" Version="4.1.1" />
  </ItemGroup>
  
</Project>
